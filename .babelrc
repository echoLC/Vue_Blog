{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 2
      }
    ]
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      },
      {
        "libraryName": "element-ui",
        "styleLibraryName": "~theme"
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}
